{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": ["airbnb-base","prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "cypress",
        "jest"
    ],
    "rules": {
        "import/extensions": [
            "error",
            {
                "js": "always",
                "json": "sometimes"
            }
        ]
    }
}
